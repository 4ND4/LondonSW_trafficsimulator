<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="400.0" prefWidth="600.0"
          maxHeight="400.0" maxWidth="600.0"
          fx:controller="londonsw.controller.StartUpController"
          stylesheets="/londonsw/view/startup/startScreenStyles.css"
          xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnSpan="2">
         <children>
            <Label fx:id="londonSWlabel" prefHeight="50.0" prefWidth="600.0" text="LondonSW" />
            <Label fx:id="trafficSimLabel" prefHeight="70.0" prefWidth="600.0" text="Traffic Simulator" />
         </children>
      </VBox>
      <Pane prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="1" GridPane.rowSpan="2">
         <children>
            <Button fx:id="startButton" layoutX="150.0" layoutY="82.0" mnemonicParsing="false" onAction="#goToChooseModeScreen" prefHeight="150.0" prefWidth="300.0" text="Start">
               <font>
                  <Font size="30.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</GridPane>
